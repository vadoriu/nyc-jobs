    <ds:business_title>Vision Screening Coordinator, Bureau of School Health/SH Nursing Services &amp; Prof Dev</ds:business_title>
    <ds:civil_service_title>PUBLIC HEALTH ADVISER (SCHOOL</ds:civil_service_title>
    <ds:title_code_no>51195</ds:title_code_no>
    <ds:level>2</ds:level>
