    <ds:civil_service_title>ASSOCIATE PUBLIC HEALTH SANITA</ds:civil_service_title>
    <ds:title_code_no>31220</ds:title_code_no>
    <ds:level>2</ds:level>
    <ds:job_category>Policy &amp; Analysis</ds:job_category>
    <ds:full_time_part_time_indicator>F</ds:full_time_part_time_indicator>
